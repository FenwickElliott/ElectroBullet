{
  "name": "ElectroBullet",
  "version": "0.1.0",
  "description": "An Electron client for PushBullet",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . ElectroBullet --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=FenwickElliott --version-string.FileDescription=CFE --version-string.ProductName=]\"ElectroBullet\""
  },
  "repository": "https://github.com/FenwickElliott/ElectroBullet",
  "author": "Charles Fenwick Elliott",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-packager": "^10.1.0"
  }
}
